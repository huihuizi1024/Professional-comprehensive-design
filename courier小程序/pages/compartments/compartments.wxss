.container {
  padding: 40rpx;
}

.title {
  font-size: 34rpx;
  font-weight: bold;
  margin-bottom: 30rpx;
  text-align: center;
}

.empty {
  text-align: center;
  color: #999;
  margin-top: 80rpx;
}

.grid {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.cell {
  width: 48%;
  background-color: #f7f8fa;
  border-radius: 16rpx;
  padding: 30rpx 20rpx;
  margin-bottom: 24rpx;
  text-align: center;
  box-shadow: 0 4rpx 12rpx rgba(0, 0, 0, 0.05);
}

.cell-title {
  font-size: 30rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}

.cell-size {
  font-size: 24rpx;
  color: #666;
}
